{
    "port": 3000,
    "listeningInterface": "127.0.0.1",
    "scheduleUpdate": {

        "enabled": {
            "@detail": {
                "input_type": "option",
                "options": {
                    "true": true,
                    "false": false
                }
            }
        },

        "scheduleTime": {
            "@detail": {
                "input_type": "option",
                "reset_after_input": true,
                "options": {
                    "minute": {"minute": 5},
                    "hour": {"hour": 2}
                }
            }
        }

    },
    "telegram-notifications-bot-controler":{
        "bot_token": {
            "@detail": {
                "input_type": "value",
                "type": "string",
                "required": true
            }
        }
    }
}